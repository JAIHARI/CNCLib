<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="$(ProjectDir)..\SolutionItems\CNCLibStandard.targets" />
   <PropertyGroup>
	<TargetFramework>net471</TargetFramework>
  <RootNamespace>$(SolutionNamespace).$(AssemblyName)</RootNamespace>
	<ApplicationIcon>Properties\CNCLib.ico</ApplicationIcon>
	<OutputType>WinExe</OutputType>
	<StartupObject>CNCLib.Wpf.WebAPI.Start.App</StartupObject>
  </PropertyGroup>
   <ItemGroup>
     <None Remove="NLog.config" />
   </ItemGroup>
   
   <ItemGroup>
   
    <EmbeddedResource Update="Properties\Resources.resx" Generator="ResXFileCodeGenerator" LastGenOutput="Resources.Designer.cs" />
    <Compile Update="Properties\Resources.Designer.cs" DesignTime="True" AutoGen="True" DependentUpon="Resources.resx" />
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>

     <ApplicationDefinition Include="App.xaml" SubType="Designer" Generator="XamlIntelliSenseFileGenerator" />

     <Content Include="NLog.config">
       <CopyToOutputDirectory>Always</CopyToOutputDirectory>
     </Content>
     <Compile Update="App.xaml.cs" DependentUpon="App.xaml" />
  </ItemGroup>
  
  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Runtime.Serialization" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Xaml" />
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="netstandard" />
  </ItemGroup>
  <ItemGroup>
    <None Include="App.config" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\Framework\Framework.Arduino\Framework.Arduino.csproj" />
    <ProjectReference Include="..\..\Framework\Framework.Dependency\Framework.Dependency.csproj" />
    <ProjectReference Include="..\..\Framework\Framework.Logging\Framework.Logging.csproj" />
    <ProjectReference Include="..\..\Framework\Framework.Mapper\Framework.Mapper.csproj" />
    <ProjectReference Include="..\..\Framework\Framework.Service.Abstraction\Framework.Service.Abstraction.csproj" />
    <ProjectReference Include="..\..\Framework\Framework.Tools\Framework.Tools.csproj" />
    <ProjectReference Include="..\..\Framework\Framework.Wpf\Framework.Wpf.csproj" />
    <ProjectReference Include="..\GCode.GUI\GCode.GUI.csproj" />
    <ProjectReference Include="..\Logic.Client\Logic.Client.csproj" />
    <ProjectReference Include="..\Logic.Contract.DTO\Logic.Contract.DTO.csproj" />
    <ProjectReference Include="..\Logic.Contract\Logic.Contract.csproj" />
    <ProjectReference Include="..\Service.WebAPI\Service.WebAPI.csproj" />
    <ProjectReference Include="..\Service.Contract\Service.Contract.csproj" />
    <ProjectReference Include="..\Shared\Shared.csproj" />
    <ProjectReference Include="..\Wpf\Wpf.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite">
      <Version>2.2.0</Version>
    </PackageReference>
    <PackageReference Include="System.Data.SQLite.Core">
      <Version>1.0.109.2</Version>
    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>