    </None>
    <None Include="Examples\spider.hpgl">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="Examples\taekwondo.hpgl">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="Examples\witch.hpgl">
